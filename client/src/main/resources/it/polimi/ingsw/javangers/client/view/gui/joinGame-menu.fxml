      <Label layoutX="226.0" layoutY="92.0" text="Tower Color" />
      <ChoiceBox fx:id="towerColor" layoutX="226.0" layoutY="109.0" prefWidth="150.0" />
      <ImageView fx:id="DRUID" fitHeight="200.0" fitWidth="135.0" layoutX="9.0" layoutY="168.0" onMouseClicked="#selectWizard">
         <image>
            <Image url="@images/MAGO%201_1.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="KING" fitHeight="200.0" fitWidth="135.0" layoutX="161.0" layoutY="169.0" onMouseClicked="#selectWizard">
         <image>
            <Image url="@images/MAGO%202_1.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="WITCH" fitHeight="200.0" fitWidth="135.0" layoutX="307.0" layoutY="168.0" onMouseClicked="#selectWizard">
         <image>
            <Image url="@images/MAGO%203_1_correzioni.jpg" />
         </image>
      </ImageView>
      <ImageView fx:id="SENSEI" fitHeight="200.0" fitWidth="135.0" layoutX="457.0" layoutY="167.0" onMouseClicked="#selectWizard">
